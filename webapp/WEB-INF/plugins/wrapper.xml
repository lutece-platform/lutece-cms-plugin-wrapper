<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    
    <!-- Plugin Informations -->
    <name>wrapper</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.2</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>Module de gestion des liens de type Wrapper</description>
    <provider>Mairie de Paris</provider>
    <provider-url>http://fr.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/wrapper/wrapper.png</icon-url>
    <copyright>Copyright 2001-2010 Mairie de Paris</copyright>
    <core-version-dependency>
		<min-core-version>2.3.0</min-core-version>
		<max-core-version/>	
    </core-version-dependency>
    <db-pool-required>1</db-pool-required>
    
    <!--Administration interface parameters-->
    <admin-features>	
        <admin-feature>
            <feature-id>WRAPPER_MANAGEMENT</feature-id>
            <feature-title>wrapper.adminFeature.wrapper_management.name</feature-title>
            <feature-description>wrapper.adminFeature.wrapper_management.description</feature-description>
            <feature-level>2</feature-level>
            <feature-url>jsp/admin/plugins/wrapper/ManageWrapper.jsp</feature-url>
            <feature-icon-url>images/admin/skin/plugins/wrapper/wrapper.png</feature-icon-url>
            <feature-documentation-url>jsp/admin/documentation/AdminDocumentation.jsp?doc=admin-wrapper</feature-documentation-url>
        </admin-feature>
    </admin-features>
    
    <!-- Xpage configuration -->
    <applications>
        <application>
            <application-id>wrapper</application-id>
            <application-class>fr.paris.lutece.plugins.wrapper.web.WrapperApp</application-class>
        </application>
    </applications>

    
</plug-in>